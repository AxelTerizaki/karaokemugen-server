    "langs": "^2.0.0",
    "lodash.deburr": "^4.1.0",
    "lodash.merge": "^4.6.1",
    "multer": "^1.4.1",
    "nodemailer": "^6.1.0",
    "os-locale": "^3.1.0",
    "passport": "^0.4.0",
