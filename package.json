{
   "name": "karaokemugen-server",
   "version": "0.9.1",
   "description": "Karaoke Mugen online server",
   "main": "dist/index.js",
   "repository": "https://lab.shelter.moe/karaokemugen/karaokemugen-server.git",
   "author": "Guillaume Lebigot <axel@teri-chan.net>",
   "license": "MIT",
   "homepage": "https://mugen.karaokes.moe",
   "private": true,
   "workspaces": {
      "packages": [
          "kmexplorer",
		  "kmserver-core"
      ]
   },
   "scripts": {
      "migrate": "db-migrate sync [all] --config app/database.json",
      "migrateDown": "db-migrate down --config app/database.json --migrations-dir kmserver-core/migrations",
      "migrateUp": "db-migrate up --config app/database.json --migrations-dir kmserver-core/migrations",
      "buildAll": "yarn build && yarn buildKmexplorer",
      "pull": "git pull && git submodule sync --recursive && git submodule update --init --recursive"
   },
   "bugs": {
      "url": "https://lab.shelter.moe/karaokemugen/karaokemugen-server/issues",
      "email": "axel@teri-chan.net"
   },
   "engines": {
      "node": ">=10.11.0"
   }
}
