{
  "name": "karaokemugen-server",
  "version": "0.7.0",
  "description": "Karaoke Mugen online server",
  "main": "src/index.js",
  "repository": "https://lab.shelter.moe/karaokemugen/karaokemugen-server.git",
  "author": "Guillaume Lebigot <axel@teri-chan.net>",
  "license": "MIT",
  "homepage": "https://mugen.karaokes.moe",
  "scripts": {
    "prestart": "node node_modules/db-migrate/bin/db-migrate sync [all]",
    "start": "babel-node src/index.js --",
    "generate": "babel-node src/index.js -- --generate",
    "migrate": "node node_modules/db-migrate/bin/db-migrate sync [all]",
    "migrateDown": "node node_modules/db-migrate/bin/db-migrate down",
    "migrateUp": "node node_modules/db-migrate/bin/db-migrate up",
    "build": "rm -Rf dist && babel --source-maps --copy-files -d dist/ src/ && yarn buildKmimport && yarn buildKmexplorer",
    "install": "cd kmimport && yarn install && cd ../kmexplorer && yarn install",
    "buildKmimport": "cd kmimport && yarn build",
    "buildKmexplorer": "cd kmexplorer && rm -Rf .next && npx next build"
  },
  "bugs": {
    "url": "https://lab.shelter.moe/karaokemugen/karaokemugen-server/issues",
    "email": "axel@teri-chan.net"
  },
  "engines": {
    "node": ">=10.11.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^5.16.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-flowtype": "^3.6.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-security": "^1.4.0"
  },
  "dependencies": {
    "async-await-parallel": "^1.0.0",
    "body-parser": "^1.18.3",
    "commander": "^2.20.0",
    "db-migrate": "^0.11.5",
    "db-migrate-pg": "^0.5.0",
    "detect-port": "^1.3.0",
    "execa": "^1.0.0",
    "express": "^4.16.4",
    "express-http-proxy": "^1.5.1",
    "express-range": "^2.0.1",
    "file-type": "^10.10.0",
    "find-free-port": "^2.0.0",
    "find-remove": "^1.2.2",
    "fs-extra": "^7.0.1",
    "got": "^9.6.0",
    "helmet": "^3.16.0",
    "i18n": "^0.8.3",
