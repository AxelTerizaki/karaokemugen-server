{
  "name": "karaokemugen-server",
  "version": "1.0.0",
  "description": "Karaoke Mugen online server",
  "main": "src/index.js",
  "repository": "https://lab.shelter.moe/karaokemugen/karaokemugen-server.git",
  "author": "Guillaume Lebigot <axel@teri-chan.net>",
  "license": "MIT",
  "homepage": "https://mugen.karaokes.moe",
  "scripts": {
    "prestart": "node node_modules/db-migrate/bin/db-migrate sync [all] --migrations-dir src/_dao/migrations",
    "start": "babel-node src/index.js --",
    "buildReact": "cd react_site && yarn build",
    "startReact": "cd react_site && yarn start",
    "generate": "babel-node src/index.js -- --generate",
    "migrate": "node node_modules/db-migrate/bin/db-migrate sync [all] --migrations-dir src/_dao/migrations",
    "migrateDown": "node node_modules/db-migrate/bin/db-migrate down --migrations-dir src/_dao/migrations",
    "migrateUp": "node node_modules/db-migrate/bin/db-migrate up --migrations-dir src/_dao/migrations"
  },
  "bugs": {
    "url": "https://lab.shelter.moe/karaokemugen/karaokemugen-server/issues",
    "email": "axel@teri-chan.net"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^5.1.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "^7.10.0"
  },
