{
   "name": "karaokemugen-server",
   "version": "0.9.0",
   "description": "Karaoke Mugen online server",
   "main": "dist/index.js",
   "repository": "https://lab.shelter.moe/karaokemugen/karaokemugen-server.git",
   "author": "Guillaume Lebigot <axel@teri-chan.net>",
   "license": "MIT",
   "homepage": "https://mugen.karaokes.moe",
   "scripts": {
      "prestart": "node node_modules/db-migrate/bin/db-migrate sync [all] --config app/database.json",
      "start": "ts-node src/index.ts --",
      "qstart": "yarn prestart && node dist/index.js --",
      "generate": "ts-node src/index.ts -- --generate",
      "migrate": "db-migrate sync [all] --config app/database.json",
      "migrateDown": "db-migrate down --config app/database.json",
      "migrateUp": "db-migrate up --config app/database.json",
      "build": "npx rimraf \"dist\" && tsc --build",
      "buildAll": "yarn build && yarn buildKmimport && yarn buildKmexplorer",
      "installAll": "yarn install && cd kmimport && yarn install && cd ../kmexplorer && yarn install",
      "buildKmimport": "cd kmimport && yarn build",
      "buildKmexplorer": "cd kmexplorer && npx rimraf \".next\" && npx next build",
      "pull": "git pull && git submodule sync --recursive && git submodule update --init --recursive"
   },
   "bugs": {
      "url": "https://lab.shelter.moe/karaokemugen/karaokemugen-server/issues",
      "email": "axel@teri-chan.net"
   },
