{
  "name": "karaokemugen-server",
  "version": "0.9.0",
  "description": "Karaoke Mugen online server",
  "main": "dist/index.js",
  "repository": "https://lab.shelter.moe/karaokemugen/karaokemugen-server.git",
  "author": "Guillaume Lebigot <axel@teri-chan.net>",
  "license": "MIT",
  "homepage": "https://mugen.karaokes.moe",
  "scripts": {
    "prestart": "node node_modules/db-migrate/bin/db-migrate sync [all]",
    "start": "ts-node src/index.ts --",
    "qstart": "yarn prestart && node dist/index.js --",
    "generate": "ts-node src/index.ts -- --generate",
    "migrate": "node node_modules/db-migrate/bin/db-migrate sync [all]",
    "migrateDown": "node node_modules/db-migrate/bin/db-migrate down",
    "migrateUp": "node node_modules/db-migrate/bin/db-migrate up",
    "build": "rm -Rf dist && tsc --build",
    "buildAll": "rm -Rf dist && yarn build && yarn buildKmimport && yarn buildKmexplorer",
    "installAll": "cd kmimport && yarn install && cd ../kmexplorer && yarn install",
    "buildKmimport": "cd kmimport && yarn build",
    "buildKmexplorer": "cd kmexplorer && rm -Rf .next && npx next build",
    "pull": "git pull && git submodule sync --recursive && git submodule update --init --recursive"
  },
  "bugs": {
    "url": "https://lab.shelter.moe/karaokemugen/karaokemugen-server/issues",
    "email": "axel@teri-chan.net"
  },
  "engines": {
    "node": ">=10.11.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/detect-port": "^1.1.0",
    "@types/execa": "^0.9.0",
    "@types/fs-extra": "^7.0.0",
    "@types/got": "^9.6.0",
    "@types/helmet": "^0.0.43",
    "@types/i18n": "^0.8.5",
    "@types/ini": "^1.3.30",
    "@types/js-yaml": "^3.12.1",
    "@types/lodash.deburr": "^4.1.6",
    "@types/lodash.merge": "^4.6.6",
    "@types/multer": "^1.3.7",
    "@types/nodemailer": "^6.2.0",
    "@types/os-locale": "^2.1.0",
    "@types/passport": "^1.0.0",
    "@types/passport-jwt": "^3.0.1",
    "@types/pg": "^7.4.14",
    "@types/pg-copy-streams": "^1.2.0",
    "@types/randomstring": "^1.1.6",
    "@types/sanitize-filename": "^1.1.28",
    "@types/socket.io-client": "^1.4.32",
    "@types/uuid": "^3.4.4",
    "@types/yesql": "^3.2.1",
    "eslint": "^5.16.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-flowtype": "^3.10.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-security": "^1.4.0",
    "ts-node": "^8.2.0",
    "typescript": "^3.5.1"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "commander": "^2.20.0",
    "db-migrate": "^0.11.6",
    "db-migrate-pg": "^1.0.0",
    "detect-port": "^1.3.0",
    "execa": "^1.0.0",
    "express": "^4.17.1",
    "express-http-proxy": "^1.5.1",
    "express-range": "^2.0.1",
    "find-remove": "^1.2.2",
    "helmet": "^3.18.0",
    "i18n": "^0.8.3",
    "is-valid-json": "^1.0.2",
    "iso-countries-languages": "^0.2.1",
    "jwt-simple": "^0.5.6",
    "karaokemugen-libs": "file:./src/lib/",
    "langs": "^2.0.0",
    "multer": "^1.4.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
