		"start": "cross-env NODE_ENV=production ts-node src/index.ts --",
		"watch": "yarn prestart && cross-env NODE_ENV=development ts-node src/index.ts --",
		"qstart": "yarn prestart && node dist/index.js --",
		"generate": "ts-node src/index.ts -- --generate",
		"build": "rimraf \"dist\" && cross-env NODE_ENV=production tsc --build",
		"buildKMExplorer": "rimraf \"../kmexplorer/.nuxt\" && cross-env NODE_ENV=production ts-node src/index.ts -- --build"
